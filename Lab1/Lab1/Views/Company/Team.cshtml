
@{
    ViewBag.Title = "Team";
}
<div>
    <a href="/">Home</a>
    <a href="/Company/Services">Services</a>
    <a href="/Company/Team">Team</a>
    <a href="/Company/Contact">Contact Us</a>

</div>

<h2>Team</h2>
<ol>
    @foreach (var item in Model)
    {
        <li>
            @item.Name
            <ul>
                <li>@item.Age </li>
                <li>@item.Role</li>
            </ul>
        </li>
    }
</ol>
